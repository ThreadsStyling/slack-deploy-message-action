{
  "name": "slack-deploy-message-action",
  "version": "0.0.0",
  "private": true,
  "description": "This action will send a nice deploy message in slack",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.5.11",
    "@vercel/ncc": "^0.38.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "typescript": "^5.4.5"
  }
}
